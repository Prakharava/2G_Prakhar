package ASSIGNMENT1;

import java.util.*;
public class Farenhiet {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int minimum = sc.nextInt();
        int maximum = sc.nextInt();
        int step = sc.nextInt();
        int number = 0;
        int f = minimum;
        for (; f <= maximum; f+=step){
            int rem =(f-32)*5/9;
            number = rem;
            System.out.println(f+" "+rem);
        }
        sc.close();
    }
}
